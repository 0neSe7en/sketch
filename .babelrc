{
  "presets": [
    [
      "env",
      {
        "targets": {
          "node": "current"
        }
      }
    ],
    "react",
    "stage-0"
  ],
  "plugins": [
    "transform-decorators-legacy",
    [
      "transform-builtin-extend",
      {
        "globals": ["Error", "Array"]
      }
    ],
    [
      "babel-plugin-styled-components",
      {
        "displayName": true,
        "fileName": false,
        "pure": true,
        "ssr": false
      }
    ]
  ]
}
