{
  "name": "@36node/fastman",
  "version": "0.0.1",
  "description": "A tool to export and import postman collection file",
  "repository": {
    "url": "undefined/undefined",
    "type": "git"
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.js",
  "umd:main": "dist/index.umd.js",
  "module": "dist/index.mjs",
  "files": [
    "src",
    ".circleci",
    "README.md"
  ],
  "bin": {
    "fastman": "dist/cli.js"
  },
  "scripts": {
    "build": "microbundle build --format cjs src/{cli,index}.js",
    "dev": "microbundle build -w --format cjs src/{cli,index}.js",
    "format": "prettier --trailing-comma es5 --write",
    "lint": "eslint src",
    "release": "standard-version && git push --follow-tags origin master",
    "test": "jest --silent"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e $GIT_PARAMS",
      "pre-commit": "lint-staged",
      "pre-push": "yarn lint && yarn test"
    }
  },
  "jest": {
    "testEnvironment": "node"
  },
  "lint-staged": {
    "*.js": [
      "yarn format",
      "git add"
    ]
  },
  "dependencies": {
    "@36node/fetch": "^0.3.4",
    "commander": "^2.16.0",
    "http-errors": "^1.7.1",
    "jsonfile": "^5.0.0"
  },
  "devDependencies": {
    "@36node/babel": "^0.1.0",
    "@36node/eslint-config": "^0.1.0",
    "@36node/sketch-cli": "^0.4.2",
    "@commitlint/cli": "^6.1.0",
    "@commitlint/config-conventional": "^6.1.0",
    "husky": "^1.0.0-rc.13",
    "jest": "^23.4.2",
    "lint-staged": "^7.2.0",
    "microbundle": "^0.6.0",
    "prettier": "^1.13.7",
    "standard-version": "^4.4.0"
  },
  "gitHead": "3d6769747b439025d412c685aaaec136c4f18248"
}
